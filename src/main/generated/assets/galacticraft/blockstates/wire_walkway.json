{
  "multipart": [
    {
      "apply": {
        "model": "galacticraft:block/aluminum_wire"
      }
    },
    {
      "apply": {
        "model": "galacticraft:block/walkway"
      },
      "when": {
        "facing": "up"
      }
    },
    {
      "apply": {
        "model": "galacticraft:block/walkway",
        "x": 180
      },
      "when": {
        "facing": "down"
      }
    },
    {
      "apply": {
        "model": "galacticraft:block/walkway",
        "x": 90
      },
      "when": {
        "facing": "north"
      }
    },
    {
      "apply": {
        "model": "galacticraft:block/walkway",
        "x": 270
      },
      "when": {
        "facing": "south"
      }
    },
    {
      "apply": {
        "model": "galacticraft:block/walkway",
        "x": 90,
        "y": 90
      },
      "when": {
        "facing": "east"
      }
    },
    {
      "apply": {
        "model": "galacticraft:block/walkway",
        "x": 270,
        "y": 90
      },
      "when": {
        "facing": "west"
      }
    },
    {
      "apply": {
        "model": "galacticraft:block/walkway_center"
      },
      "when": {
        "facing": "up",
        "up": "false"
      }
    },
    {
      "apply": {
        "model": "galacticraft:block/walkway_center",
        "x": 180
      },
      "when": {
        "down": "false",
        "facing": "down"
      }
    },
    {
      "apply": {
        "model": "galacticraft:block/walkway_center",
        "x": 90
      },
      "when": {
        "facing": "north",
        "north": "false"
      }
    },
    {
      "apply": {
        "model": "galacticraft:block/walkway_center",
        "x": 270
      },
      "when": {
        "facing": "south",
        "south": "false"
      }
    },
    {
      "apply": {
        "model": "galacticraft:block/walkway_center",
        "x": 90,
        "y": 90
      },
      "when": {
        "east": "false",
        "facing": "east"
      }
    },
    {
      "apply": {
        "model": "galacticraft:block/walkway_center",
        "x": 270,
        "y": 90
      },
      "when": {
        "facing": "west",
        "west": "false"
      }
    }
  ]
}